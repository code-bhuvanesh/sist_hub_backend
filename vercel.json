{
    "build": [
      {
        "src": "sist_hub_server/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sist_hub_server/wsgi.py"
      }
    ]
  }
